int processData()
{
    int imgToShow = 0;
    bool vlaga = false;
    bool underWater = !digitalRead(poplava);
    dhtError += readSensor();
    if(dhtError > 5){
      imgToShow = 7;
      return imgToShow;
    }

    if(tempDHT > maxTempLimit){
      imgToShow = 8;
      return imgToShow;
    }else if(tempDHT < minTempLimit){
      imgToShow = 9;
      return imgToShow;
    }else if(humDHT > maxHumLimit){
      imgToShow = 10;
      return imgToShow;
    }
    
    if (humDHT > maxhum && !underWater)
    {
      if (digitalRead(heater)==0)
        {
          digitalWrite(heater, HIGH);
          debugln("HEATON");
        }
      if (digitalRead(vent)==0){
          digitalWrite(vent,HIGH);  
          debugln("VENTON");
        }
        imgToShow = 5;
        vlaga = true;
    }else if (humDHT <= maxhum)
    {
      vlaga = false;
    }


    if (tempDHT < mintemp && !vlaga)
    {
        imgToShow = 1;
        if (digitalRead(heater)==0)
        {
          digitalWrite(heater, HIGH);
          debugln("HEATON");
        }
    }else
    if (tempDHT >= mintemp && !vlaga)
    {
      imgToShow = 0;
      if (digitalRead(heater) == 1)
      {
          digitalWrite(heater, LOW);
          debugln("HEATOFF");  
      }
    }
    if (tempDHT > maxtemp && !vlaga)
    {
        imgToShow = 2;
        if (digitalRead(vent)==0){
          digitalWrite(vent,HIGH);  
          debugln("VENTON");
        }
    }else
    if (tempDHT <= maxtemp && !vlaga)
    {
      if(digitalRead(heater)==0){
        imgToShow = 0;  
      }
        if (digitalRead(vent)==1){
        digitalWrite(vent,LOW);  
        debugln("VENTOFF");
        }
    }
    if(onBattery){
      imgToShow = 4;
    }
    if(underWater){
      imgToShow = 6;
      digitalWrite(heater, LOW);
      digitalWrite(vent, LOW);
    }
    debug("H: ");
    debug(humDHT);
    debug("% ");
    debug(" T: ");
    debug(tempDHT);
    debugln("C");
    debugln(String("VentStatus: ")+String(digitalRead(vent)));
    if(sendData){
      byte toSend[7] = {I2CAddress::Interface, digitalRead(heater), digitalRead(vent), underWater, tempDHT, humDHT, sendData};
      Wire.beginTransmission(I2CAddress::Controller);
      for(int i = 0; i < 7;i++){
        Wire.write(toSend[i]);
      }
      Wire.endTransmission();
      // imgToShow = 3;
      sendData = false;
    }
    if((digitalRead(heater) || heaterManual) && !underWater){
      countHeaterStatus++;
      digitalWrite(heaterSwitch, HIGH);
    }else{
      countHeaterStatus++;
      digitalWrite(heaterSwitch, LOW);
    }
    if((digitalRead(vent) || coolerManual) && !underWater){
      countVentStatus++;
      digitalWrite(ventSwitch, HIGH);
    }else{
      countVentStatus++;
      digitalWrite(ventSwitch, LOW);
    }
    if(millis() - prevMillisOfStatus >= 5000){
      countHeaterStatus = 0;
      countVentStatus = 0;
      prevMillisOfStatus = millis();
    }else{
      if(countHeaterStatus > 5 && countVentStatus < 5){
        maxtemp += 5;
      }
      if(countVentStatus > 5 && countHeaterStatus < 5){
        mintemp -= 5;
      }
      if(countHeaterStatus > 5 && countVentStatus > 5){
        maxhum += 5;
      }
    }
    return imgToShow;
 }



bool readSensor()
{
  // Make sure we don't poll the sensor too often
  // - Max sample rate DHT11 is 1 Hz   (duty cicle 1000 ms)
  // - Max sample rate DHT22 is 0.5 Hz (duty cicle 2000 ms)
  if(millis() - prevMillis >= 1000){
    prevMillis = millis();
  }else{
    return false;
  }
  unsigned long startTime = millis();
  // Request sample

  digitalWrite(DHT11Pin, LOW); // Send start signal
  pinMode(DHT11Pin, OUTPUT);
  delay(18);

  pinMode(DHT11Pin, INPUT);
  digitalWrite(DHT11Pin, HIGH); // Switch bus to receive data

  // We're going to read 83 edges:
  // - First a FALLING, RISING, and FALLING edge for the start bit
  // - Then 40 bits: RISING and then a FALLING edge per bit
  // To keep our code simple, we accept any HIGH or LOW reading if it's max 85 usecs long

  uint16_t rawHumidity = 0;
  uint16_t rawTemperature = 0;
  uint16_t data = 0;

  for ( int8_t i = -3 ; i < 2 * 40; i++ ) {
    byte age;
    startTime = micros();

    do {
      age = (unsigned long)(micros() - startTime);
      if ( age > 90 ) {
        //error = ERROR_TIMEOUT;
        return true;
      }
    }
    while ( digitalRead(DHT11Pin) == (i & 1) ? HIGH : LOW );

    if ( i >= 0 && (i & 1) ) {
      // Now we are being fed our 40 bits
      data <<= 1;

      // A zero max 30 usecs, a one at least 68 usecs.
      if ( age > 30 ) {
        data |= 1; // we got a one
      }
    }

    switch ( i ) {
      case 31:
        rawHumidity = data;
        break;
      case 63:
        rawTemperature = data;
        data = 0;
        break;
    }
  }

  if ( (byte)(((byte)rawHumidity) + (rawHumidity >> 8) + ((byte)rawTemperature) + (rawTemperature >> 8)) != data ) {
    //error = ERROR_CHECKSUM;
    return true;
  }

  // Store readings

  humDHT = rawHumidity >> 8;
  tempDHT = rawTemperature >> 8;
  dhtError = 0;
  return false;
}

